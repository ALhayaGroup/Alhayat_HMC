<odoo>
    <record id="view_get_payslip_form" model="ir.ui.view">
        <field name="name">get.payslip.form</field>
        <field name="model">get.payslip</field>
        <field name="arch" type="xml">
            <form string="Get payslip">
            
                <group>
                    <field name="date_from"/>
                    
                </group>
                <group >
                    <field name="date_to"/>
                </group>`
                
                <footer>
                
                    <button name="get_data" type="object" class="btn-primary"  string="get payslip"   />
                </footer>
            </form>
        </field>
    </record>

    <record id="action_get_payslip" model="ir.actions.act_window">
        <field name="name">Get payslip</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">get.payslip</field>
        <field name="view_mode">form</field>
        <field name="view_id" ref="view_get_payslip_form"/>
        <field name="target">new</field>
      
    </record>

    <menuitem
        id="get_payslip_menuitem"
        name="Get payslip"
        action="action_get_payslip"
                
        
        parent="om_hr_payroll.menu_hr_payroll_root"
        sequence="20"/>
</odoo>
